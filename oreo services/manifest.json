{
  "manifest_version": 3,
  "name": "Oreo Swapper",
  "version": "1.3",
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "OreoIcon.png",
      "48": "OreoIcon.png",
      "128": "OreoIcon.png"
    }
  },
  "icons": {
    "16": "OreoIcon.png",
    "48": "OreoIcon.png",
    "128": "OreoIcon.png"
  }
}
